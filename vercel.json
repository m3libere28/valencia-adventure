{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "buildCommand": "npm install",
    "outputDirectory": "public",
    "framework": null,
    "installCommand": "npm install"
}
